{
  "name": "sber-react-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "eslint:fix": "npx eslint --fix --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx,md,html}",
    "eslint": "npx eslint --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx,md,html}",
    "stylelint": "npx stylelint src/**/*.css",
    "stylelint:fix": "npx stylelint --fix src/**/*.css",
    "lintall": "npm run eslint & npm run stylelint",
    "lintall:fix": "npm run eslint:fix & npm run stylelint:fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html}": [
      "npm run eslint",
      "npx prettier --write --single-quote"
    ],
    "*.css": [
      "npm run stylelint",
      "npx prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0"
  }
}
